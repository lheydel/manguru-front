{
  "name": "manguru-front",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run tslint && react-scripts start",
    "build": "npm run tslint && react-scripts build",
    "test": "react-scripts test --coverage a",
    "eject": "react-scripts eject",
    "jest": "jest --forceExit --detectOpenHandles --coverage --verbose",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "connected-react-router": "^6.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.4"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-test-renderer": "^16.8.2",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-thunk": "^2.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-sonar-reporter": "^2.0.0",
    "react-test-renderer": "^16.8.6",
    "redux-devtools": "^3.5.0",
    "tslint": "^5.18.0",
    "typescript": "^3.4.3",
    "utility-types": "^3.7.0"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-report.xml",
    "indent": 4
  }
}
